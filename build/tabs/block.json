{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "prc-block/tabs",
  "title": "Tabs",
  "version": "1.0.0",
  "category": "design",
  "allowedBlocks": [
    "prc-block/tab"
  ],
  "attributes": {
    "defaultActiveTabIndex": {
      "type": "number",
      "default": 0
    },
    "orientation": {
      "type": "string",
      "default": "horizontal",
      "enum": [
        "horizontal",
        "vertical"
      ]
    },
    "tabBackgroundColor": {
      "type": "string"
    },
    "customTabBackgroundColor": {
      "type": "string"
    },
    "tabHoverColor": {
      "type": "string"
    },
    "customTabHoverColor": {
      "type": "string"
    },
    "tabActiveColor": {
      "type": "string"
    },
    "customTabActiveColor": {
      "type": "string"
    },
    "tabTextColor": {
      "type": "string"
    },
    "customTabTextColor": {
      "type": "string"
    },
    "tabActiveTextColor": {
      "type": "string"
    },
    "customTabActiveTextColor": {
      "type": "string"
    },
    "tabHoverTextColor": {
      "type": "string"
    },
    "customTabHoverTextColor": {
      "type": "string"
    }
  },
  "example": {
    "attributes": {
      "className": "is-example"
    },
    "innerBlocks": [
      {
        "name": "prc-block/tab",
        "attributes": {
          "label": "Tab 1",
          "slug": "tab-1",
          "tabIndex": 0
        },
        "innerBlocks": [
          {
            "name": "core/paragraph",
            "attributes": {
              "content": "Tab 1 content"
            }
          }
        ]
      },
      {
        "name": "prc-block/tab",
        "attributes": {
          "label": "Tab 2",
          "slug": "tab-2",
          "tabIndex": 1
        }
      },
      {
        "name": "prc-block/tab",
        "attributes": {
          "label": "Tab 3",
          "slug": "tab-3",
          "tabIndex": 2
        }
      }
    ]
  },
  "styles": [
    {
      "name": "tabbed",
      "label": "Tabbed",
      "isDefault": true
    },
    {
      "name": "tabbed-outline",
      "label": "Tabbed Outline"
    },
    {
      "name": "underlined",
      "label": "Underlined"
    },
    {
      "name": "pills",
      "label": "Pills"
    },
    {
      "name": "underlined-pills",
      "label": "Underlined Pills"
    }
  ],
  "supports": {
    "align": true,
    "color": {
      "text": false,
      "background": false
    },
    "html": false,
    "interactivity": true,
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "fontFamily": true
    }
  },
  "textdomain": "tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScriptModule": "file:./view.js"
}