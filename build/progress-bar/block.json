{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "prc-block/progress-bar",
  "version": "0.1.0",
  "title": "Progress Bar",
  "category": "media",
  "icon": "chart-bar",
  "keywords": [
    "progress",
    "bar",
    "quiz",
    "result",
    "horizontal",
    "chart"
  ],
  "attributes": {
    "barColor": {
      "type": "string",
      "default": "social-trends-teal"
    },
    "valueColor": {
      "type": "string",
      "default": "ui-black"
    },
    "backgroundColor": {
      "type": "string",
      "default": "ui-beige-very-light"
    },
    "barHeight": {
      "type": "number",
      "default": 10
    },
    "maxValue": {
      "type": "number",
      "default": 100
    },
    "label": {
      "type": "string",
      "source": "html",
      "selector": "label",
      "default": "Progress"
    },
    "value": {
      "type": "number",
      "default": 0
    },
    "labelFormat": {
      "type": "string",
      "default": "percentage",
      "enum": [
        "percentage",
        "fractional"
      ]
    },
    "labelPosition": {
      "type": "string",
      "default": "left",
      "enum": [
        "left",
        "right"
      ]
    },
    "valuePosition": {
      "type": "string",
      "default": "outside",
      "enum": [
        "inside",
        "outside"
      ]
    },
    "valueFontSize": {
      "type": "number",
      "default": 12
    }
  },
  "supports": {
    "anchor": true,
    "html": false,
    "interactivity": true,
    "animations": true,
    "color": {
      "background": true,
      "text": true,
      "__experimentalSkipSerialization": true
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "__experimentalDefaultControls": {
        "padding": true
      }
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalFontFamily": true
    }
  },
  "example": {
    "attributes": {
      "label": "Progress",
      "value": 35,
      "maxValue": 100
    }
  },
  "textdomain": "progress-bar",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "viewScriptModule": "file:./view.js"
}