{
    "apiVersion": 2,
	"name": "prc-block/github-gist",
	"category": "embed",
	"attributes": {
		"url": {
			"type": "string"
		},
		"file": {
			"type": "string"
		},
		"meta": {
			"type": "boolean",
			"default": true
		},
		"caption": {
			"type": "string",
			"source": "html",
			"selector": "figcaption"
		}
	},
    "supports": {
        "html": false,
        "align": ["wide"]
    }
}
