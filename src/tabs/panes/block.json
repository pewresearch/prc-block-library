{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
	"name": "prc-block/tabs-panes",
	"title": "Panes",
	"description": "Contains the pane block that contains content for each tab.",
    "category": "layout",
    "attributes": {},
    "parent": ["prc-block/tabs"],
    "supports": {
        "html": false,
        "align": false,
        "inserter": false,
        "multiple": false,
		"color": {
			"background": true,
			"text": true
		},
		"spacing": {
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"__experimentalFontFamily": true,
			"__experimentalDefaultControls": {
				"fontSize": true,
				"__experimentalFontFamily": true
			}
		}
    },
    "usesContext": ["prc-block/tabs/layout", "prc-block/tabs/active"]
}
