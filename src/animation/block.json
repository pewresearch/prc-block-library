{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "prc-block/animation",
	"title": "Animation",
	"version": "1.0.0",
	"category": "design",
	"attributes": {
		"animation": {
			"type": "string",
			"enum": [
				"confetti",
				"emoji"
			]
		},
		"emoji": {
			"type": "string"
		},
		"effect": {
			"type": "string",
			"enum": [
				"center",
				"fireworks",
				"rain"
			]
		},
		"speed": {
			"type": "number",
			"default": 5000
		}
	},
	"supports": {
		"anchor": true,
		"html": false,
		"reusable": true,
		"interactivity": true,
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"textdomain": "animation",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"viewScriptModule": "file:./view.js"
}
