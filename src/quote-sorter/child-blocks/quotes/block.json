{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "prc-block/quote-sorter-quotes",
	"title": "Quote Sorter â€“ Container",
	"icon": "list-view",
	"description": "Child block containing a group of quotes",
	"category": "layout",
	"attributes": {
		"style": {
			"type": "object",
			"default": {
				"color": {
					"background": "#ffffff",
					"text": "#2a2a2a"
				},
				"typography": {
					"fontSize": 16,
					"lineHeight": 1.5
				}
			}
		},
		"marginTop": {
			"type": "number",
			"default": 20
		},
		"marginBottom": {
			"type": "number",
			"default": 20
		},
		"includeQuoteArt": {
			"type": "boolean",
			"default": true
		},
		"noResultsMessage": {
			"type": "string",
			"default": "No results found. Please try another search."
		},
		"customAttributionStyles": {
			"type": "boolean",
			"default": false
		},
		"attributionStyles": {
			"type": "object",
			"default": {
				"color": "#000000",
				"fontSize": "16px",
				"fontWeight": "400",
				"fontFamily": "inherit",
				"lineHeight": "1.5",
				"textTransform": "none",
				"textDecoration": "none",
				"textAlign": "center"
			}
		}
	},
	"styles": [
		{
			"name": "block",
			"label": "Block",
			"isDefault": true
		},
		{
			"name": "columns",
			"label": "Columns"
		}
	],
	"supports": {
		"color": {
			"background": true,
			"text": true
		},
		"__experimentalBorder": {
			"color": true,
			"width": true
		},
		"html": false,
		"reusable": false,
		"spacing": {
			"padding": true,
			"margin": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true
		}
	},
	"usesContext": ["prc-block/quote-sorter-hash"],
	"ancestor": ["prc-block/quote-sorter"],
	"providesContext": {
		"prc-block/quote-sorter-art": "includeQuoteArt",
		"prc-block/quote-sorter-attribution-styles": "attributionStyles"
	}
}
