{
	"apiVersion": 2,
	"name": "prc-block/progress-bar",
	"category": "layout",
	"attributes": {
		"maxWidth": {
			"type": "number",
			"default": "640"
		},
		"barColor": {
			"type": "string",
			"default": "#436983"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#F1F1F1"
		},
		"maxValue": {
			"type": "number",
			"default": 100
		},
		"currentValue": {
			"type": "number",
			"default": 50
		},
		"showAxisLabel": {
			"type": "boolean",
			"default": true
		},
		"axisLabel": {
			"type": "string",
			"default": "Total"
		},
		"axisPadding": {
			"type": "number",
			"default": 60
		},
		"labelFormat": {
			"type": "string",
			"default": "fractional"
		},
		"labelPositionDX": {
			"type": "integer",
			"default": -45
		},
		"labelPositionDY": {
			"type": "integer",
			"default": -2
		}
	},
	"supports": {
		"html": false
	},
	"usesContext": ["prc-block/hasDarkBackground"]
}
