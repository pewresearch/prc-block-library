{
    "apiVersion": 2,
	"name": "prc-block/github-gist",
	"title": "GitHub Gist",
	"description": "Embed a Gist from GitHub",
	"category": "embed",
	"keywords": [ "gist", "code", "github", "python", "javascript" ],
	"attributes": {
		"url": {
			"type": "string"
		},
		"file": {
			"type": "string"
		},
		"meta": {
			"type": "boolean",
			"default": true
		},
		"caption": {
			"type": "string",
			"source": "html",
			"selector": "figcaption"
		}
	},
    "supports": {
        "html": false,
        "align": ["wide", "left", "right", "center"]
    }
}
