{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "prc-block/taxonomy-navigation",
	"version": "0.1.0",
	"title": "Taxonomy Navigation",
	"category": "design",
	"description": "Display a list of terms from a taxonomy in a structured nestable list.",
	"attributes": {
		"allowedBlocks": {
			"type": "array"
		},
		"heading": {
			"type": "string"
		},
		"taxonomy": {
			"type": "string"
		}
	},
	"supports": {
		"anchor": true,
		"html": false,
		"reusable": true,
		"spacing": {
			"blockGap": true,
			"margin": [
				"top",
				"bottom"
			],
			"padding": true,
			"__experimentalDefaultControls": {
				"padding": true
			}
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true,
				"fontAppearance": true,
				"textTransform": true
			}
		}
	},
	"providesContext": {
		"taxonomy-navigation/taxonomy": "taxonomy"
	},
	"textdomain": "taxonomy-navigation",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
