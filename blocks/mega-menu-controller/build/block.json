{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "prc-block/mega-menu-controller",
  "version": "0.1.0",
  "title": "Mega Menu",
  "category": "theme",
  "attributes": {
    "label": {
      "type": "string",
      "default": "Research Topics"
    },
    "activeColor": {
      "type": "string"
    },
    "activeBorderColor": {
      "type": "string"
    }
  },
  "supports": {
    "anchor": true,
    "html": false,
    "spacing": {
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontAppearance": true,
        "textTransform": true
      }
    }
  },
  "usesContext": [
    "menu/className",
    "menu/taxonomy",
    "menu/activeColor",
    "menu/textColor",
    "menu/backgroundColor",
    "menu/hoverBackgroundColor",
    "menu/borderColor",
    "style"
  ],
  "ancestor": [
    "prc-block/menu"
  ],
  "textdomain": "mega-menu-controller",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}