{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "prc-block/embed-wrapper",
	"version": "0.1.0",
	"title": "Embed Wrapper",
	"category": "widgets",
	"description": "Wrap innerblocks in an embed footer",
	"attributes": {
		"allowedBlocks": {
			"type": "array"
		},
		"prcEmbed": {
			"type": "object",
			"default": {
				"enabled": true,
				"id": null
			}
		}
	},
	"supports": {
		"anchor": true,
		"html": false,
		"spacing": {
			"blockGap": true,
			"margin": [ "top", "bottom" ],
			"padding": true,
			"__experimentalDefaultControls": {
				"padding": true
			}
		}
	},
	"textdomain": "embed-wrapper",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
