{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "prc-block/menu-link",
	"version": "0.1.0",
	"title": "Menu Link",
	"category": "theme",
	"description": "Display a link to a taxonomy term, page, or custom URL with optional settings.",
	"attributes": {
		"label": {
			"type": "string"
		},
		"url": {
			"type": "string"
		},
		"id": {
			"type": "number"
		},
		"description": {
			"type": "string"
		},
		"title": {
			"type": "string"
		},
		"rel": {
			"type": "string"
		},
		"opensInNewTab": {
			"type": "boolean",
			"default": false
		},
		"objectType": {
			"type": "string",
			"enum": [
				"page",
				"post",
				"term",
				"url"
			],
			"default": "term"
		},
		"taxonomy": {
			"type": "string",
			"default": "topic"
		}
	},
	"ancestor": [
		"prc-block/menu"
	],
	"supports": {
		"anchor": true,
		"html": false,
		"reusable": false,
		"spacing": {
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true,
				"fontAppearance": true,
				"textTransform": true
			}
		}
	},
	"providesContext": {
		"style": "style"
	},
	"usesContext": [
		"menu/className",
		"menu/taxonomy",
		"menu/textColor",
		"menu/backgroundColor",
		"menu/hoverBackgroundColor",
		"menu/borderColor",
		"style"
	],
	"__experimentalStyle": {
		"color": {
			"text": "black",
			"link": "black"
		}
	},
	"textdomain": "menu-link",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
