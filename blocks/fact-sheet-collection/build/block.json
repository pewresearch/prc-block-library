{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "prc-block/fact-sheet-collection",
  "version": "0.1.0",
  "title": "Fact Sheet Collection",
  "category": "design",
  "description": "Display the hierarchy of the fact sheet's collection term and a link to download an associated PDF if provided.",
  "attributes": {
    "pdf": {
      "type": "object"
    },
    "altPostLabel": {
      "type": "string"
    }
  },
  "supports": {
    "anchor": true,
    "html": false,
    "color": {
      "background": true,
      "text": true,
      "link": true
    },
    "spacing": {
      "blockGap": true,
      "margin": [
        "top",
        "bottom"
      ],
      "padding": true,
      "__experimentalDefaultControls": {
        "padding": true
      }
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "__experimentalFontFamily": true
      }
    }
  },
  "example": {
    "attributes": {}
  },
  "styles": [
    {
      "name": "list",
      "label": "List",
      "isDefault": true
    },
    {
      "name": "dropdown",
      "label": "Dropdown"
    }
  ],
  "textdomain": "collection",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}