{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "prc-block/form-input-checkbox",
  "version": "0.1.0",
  "title": "Form Input Checkbox",
  "description": "A primitive block for a form input checkbox field",
  "category": "design",
  "attributes": {
    "label": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "default": "checkbox"
    },
    "value": {
      "type": "string",
      "default": ""
    },
    "defaultChecked": {
      "type": "boolean",
      "default": false
    }
  },
  "ancestor": [
    "prc-block/mailchimp-form",
    "prc-block/mailchimp-select"
  ],
  "supports": {
    "anchor": true,
    "html": false,
    "reusable": false,
    "color": {
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true
    }
  },
  "textdomain": "form-input-checkbox",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}