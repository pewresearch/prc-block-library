{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "prc-block/github-gist",
	"version": "0.1.0",
	"title": "GitHub Gist",
	"description": "Embed a Gist from GitHub",
	"category": "embed",
	"keywords": [ "gist", "code", "github", "python", "javascript" ],
	"attributes": {
		"url": {
			"type": "string",
			"default": ""
		},
		"file": {
			"type": "string",
			"default": ""
		},
		"meta": {
			"type": "boolean",
			"default": true
		},
		"caption": {
			"type": "string",
			"source": "html",
			"selector": "figcaption"
		}
	},
	"supports": {
		"html": false,
        "align": ["wide", "left", "right", "center"]
	},
	"textdomain": "github-gist",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
