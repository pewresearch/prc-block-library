{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "prc-block/taxonomy-menu-link",
	"version": "0.1.0",
	"title": "Taxonomy Menu Link",
	"category": "design",
	"description": "Display a link to a taxonomy term with the option to display a sub menu of taxonomy navigation links.",
	"attributes": {
		"allowedBlocks": {
			"type": "array"
		},
		"label": {
			"type": "string"
		},
		"url": {
			"type": "string"
		},
		"id": {
			"type": "number"
		},
		"taxonomy": {
			"type": "string",
			"default": "topic"
		},
		"enableSubMenu": {
			"type": "boolean",
			"default": false
		}
	},
	"parent": [
		"prc-block/taxonomy-menu"
	],
	"supports": {
		"anchor": true,
		"html": false,
		"reusable": true,
		"spacing": {
			"blockGap": true,
			"margin": [ "top", "bottom" ],
			"padding": true,
			"__experimentalDefaultControls": {
				"padding": true
			}
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true,
				"fontAppearance": true,
				"textTransform": true
			}
		}
	},
	"providesContext": {
		"style": "style"
	},
	"usesContext": [
		"taxonomy-menu/taxonomy",
		"taxonomy-menu/textColor",
		"taxonomy-menu/backgroundColor",
		"taxonomy-menu/hoverBackgroundColor",
		"taxonomy-menu/borderColor",
		"taxonomy-menu/fontSize",
		"taxonomy-menu/showSubMenuIcon",
		"taxonomy-menu/layout",
		"style"
	],
	"textdomain": "taxonomy-menu-link",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
