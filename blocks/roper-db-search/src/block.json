{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "prc-block/roper-db-search",
	"version": "0.1.0",
	"title": "Roper Database",
	"description": "Embeddable Roper Database search tool.",
	"keywords": ["database", "roper", "cornell", "search", "polling"],
	"category": "embed",
	"attributes": {
		"subText": {
			"type": "string",
			"default": "Use this tool to search our database of polling questions."
		},
		"perPage": {
			"type": "number",
			"default": 10
		},
		"type": {
			"type": "string",
			"default": "default"
		}
	},
	"supports": {
		"anchor": true,
		"html": false
	},
	"textdomain": "roper-db-search",
	"editorScript": "file:./index.js",
	"style": ["file:./style-index.css", "roper-db-search"],
	"render": "file:./render.php",
	"viewScript": ["file:./view.js", "roper-db-search"]
}
