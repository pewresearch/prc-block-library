{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "prc-block/taxonomy-navigation-link",
  "version": "0.1.0",
  "title": "Taxonomy Navigation Link",
  "category": "design",
  "description": "Display a link to a taxonomy term with the option to display a sub menu of taxonomy navigation links.",
  "attributes": {
    "allowedBlocks": {
      "type": "array"
    },
    "label": {
      "type": "string"
    },
    "termId": {
      "type": "number"
    },
    "taxonomy": {
      "type": "string"
    },
    "showSubmenu": {
      "type": "boolean",
      "default": false
    }
  },
  "parent": [
    "prc-block/taxonomy-navigation"
  ],
  "supports": {
    "anchor": true,
    "html": false,
    "reusable": true,
    "spacing": {
      "blockGap": true,
      "margin": [
        "top",
        "bottom"
      ],
      "padding": true,
      "__experimentalDefaultControls": {
        "padding": true
      }
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontAppearance": true,
        "textTransform": true
      }
    }
  },
  "usesContext": [
    "taxonomy-navigation/taxonomy"
  ],
  "textdomain": "taxonomy-navigation-link",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}