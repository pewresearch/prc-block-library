{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "prc-block/logo",
  "version": "1.0.0",
  "title": "Pew Research Center Logo",
  "description": "Contextually aware logo block for Pew Research Center. Can display either the standard PRC logo set or Decoded logo.",
  "category": "theme",
  "keywords": [
    "logo",
    "masthead",
    "dark-mode",
    "css-container-queries"
  ],
  "attributes": {
    "darkModeEnabled": {
      "type": "boolean",
      "default": true
    },
    "justification": {
      "type": "string",
      "default": "left",
      "enum": [
        "left",
        "center",
        "right"
      ]
    },
    "width": {
      "type": "number",
      "default": 361
    }
  },
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "gradients": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "styles": [
    {
      "name": "primary-only",
      "label": "Primary",
      "isDefault": true
    },
    {
      "name": "alt-only",
      "label": "Alternate"
    },
    {
      "name": "container-aware",
      "label": "Container Aware"
    },
    {
      "name": "decoded-only",
      "label": "Decoded"
    }
  ],
  "example": {
    "viewportWidth": 400,
    "attributes": {
      "width": 300,
      "className": "block-editor-block-types-list__prc-logo-example"
    }
  },
  "usesContext": [
    "postType",
    "postId",
    "queryId"
  ],
  "textdomain": "pewresearch-logo",
  "editorScript": "file:./index.js",
  "script": [
    "prc-icons"
  ],
  "style": "file:./style-index.css"
}